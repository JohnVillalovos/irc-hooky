{
  "hosting": {
    "public": "firebase",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://disjoint.ca/projects/irc-hooky",
        "type": 301
      },
      {
        "source": "/**",
        "destination": "https://disjoint.ca/projects/irc-hooky",
        "type": 301
      }
    ]
  }
}
